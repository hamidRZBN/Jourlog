{
  "name": "jourlog",
  "description": "gatsyby blog with typescript and tslint",
  "version": "1.0.0",
  "author": "mehrad77@gmail.com",
  "keywords": [
    "gatsby, typescript",
    "blog"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "fix": "prettier --write 'src/**/*.{ts,tsx,scss,md}' && tslint --fix 'src/**/*.{ts,tsx}' -p tsconfig.json && stylelint --fix 'src/**/*.scss' "
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.15",
    "@types/react-test-renderer": "^16.9.1",
    "@types/styled-components": "^4.4.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-gatsby": "^0.2.27",
    "gh-pages": "^2.2.0",
    "husky": "^4.0.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.0",
    "postcss-nested": "^4.2.1",
    "prettier": "^1.19.1",
    "tailwindcss": "^1.2.0-canary.5",
    "tailwindcss-dir": "^4.0.0",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "tslint-config-blvd": "^1.2.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "disqus-react": "^1.0.7",
    "gatsby": "^2.18.21",
    "gatsby-plugin-catch-links": "^2.1.24",
    "gatsby-plugin-draft": "^0.0.5",
    "gatsby-plugin-google-tagmanager": "^2.1.23",
    "gatsby-plugin-lodash": "^3.1.19",
    "gatsby-plugin-manifest": "^2.2.37",
    "gatsby-plugin-offline": "^3.0.31",
    "gatsby-plugin-postcss": "^2.1.19",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sass": "^2.1.27",
    "gatsby-plugin-sitemap": "^2.2.25",
    "gatsby-plugin-styled-components": "^3.1.17",
    "gatsby-plugin-typescript": "^2.1.26",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-autolink-headers": "^2.1.23",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-prismjs": "^3.3.30",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-remark": "^2.6.48",
    "lodash": "^4.17.15",
    "polished": "^3.4.2",
    "prismjs": "^1.19.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-test-renderer": "^16.12.0",
    "source-map-support": "latest",
    "styled-components": "^5.0.0",
    "stylelint": "^13.0.0",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended-scss": "^4.1.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.+(js|ts|tsx)": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  }
}
